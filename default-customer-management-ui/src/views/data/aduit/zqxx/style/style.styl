.page_zone {
  font-size: 14px;
  font-weight: 400;
  color: rgba(55, 57, 76, 1);
  background-color: white;
  min-height: calc(100vh - 84px);
  padding: 15px 22px;
  border-radius: 3px;
  // border: 1px solid rgba(218, 226, 237, 1);
  .title {
    font-size: 18px;
    font-weight: 600;
    display: flex;
    height: 60px;
    color: rgba(57, 60, 65, 1);
    align-items: center;
    justify-content: space-between;
    border-bottom: 1px solid rgba(218, 226, 237, 1);
    margin: 0;
    img {
      width: 26px;
      margin-right: 15px;
    }
    div {
      flex: 1;
      text-align: left;
    }
  }
  .content {
    padding: 0px 0px 22px 0px;
    .el-select, .el-input {
      width: 100%;
    }
    .content_kv {
      width: 100%;
      padding: 15px 30px;
      border-bottom: 1px dashed #DAE2ED;
      text-align: left;
        .el-col{
            margin-bottom: 10px;
            .name {
                line-height:25px;
                font-weight: 600;
                color: #666666;
                overflow: hidden;
                text-overflow: ellipsis;
                white-space: nowrap;
                color: #37394C;
            }
            div{
                color:#adadad;
            }
        }
      
    }
    .swfl_hurt_info_body, .swfl_dead_info_body {
      margin-bottom: 20px;
      border-bottom: 1px solid rgba(196, 205, 213, 0.22);
      &:last-child {
        border-bottom: none;
      }
    }
    .info_description {
      width: 100%;
      margin-top: 28px;
      .btn_add {
        float: right;
        padding-right: 20px;
      }
      & >>> .el-form-item__label {
        font-size: 14px;
        font-weight: 400 !important;
        color: rgba(55, 57, 76, 1);
      }
      .name {
        font-size: 14px;
        font-weight: 400;
        color: rgba(55, 57, 76, 1);
      }
      .text {
        font-size: 14px;
        font-weight: 400;
        color: rgba(147, 158, 171, 1);
        margin: 0;
        text-align: left;
      }
      .index {
        text-align: left;
        padding-top: 0;
        margin-top: 0;
        margin-bottom: 0;
        padding-left: 2px;
        text-align: left;
        font-size: 14px;
        font-weight: 400;
        color: rgba(147, 158, 171, 1);
      }
      .fire_building_describe {
        background: rgba(255, 255, 255, 1);
        box-shadow: 0px 3px 14px 0px rgba(0, 0, 0, 0.11);
        border-radius: 3px;
        border: 1px solid rgba(196, 205, 213, 0.22);
        margin: 0 21px 12px 23px;
        .title {
          font-size: 16px;
          font-weight: 600;
          color: rgba(57, 60, 65, 1);
          line-height: 36px;
          position: relative;
          padding-left: 18px;
          border-bottom: none;
          width: 35%;
        }
        .title::before {
          content: ' ';
          position: absolute;
          width: 6px;
          height: 18px;
          background: rgba(251, 89, 56, 1);
          left: 6px;
          top: 18px;
        }
        .radio_person {
          position: relative;
          left: 228px;
          top: -47px;
        }
      }
    }
  }
}

.audit_zone {
  .audit_top {
    display: flex;
    align-items: center;
    justify-content: space-around;
    height: 62px;
    line-height: 60px;
    background: rgba(244, 246, 248, 1);
    border-radius: 3px;
    border: 1px solid rgba(196, 205, 213, 0.22);
    margin-bottom:20px;

    div {
      display: flex;
      align-items: center;
      width: 300px;
      height: 60px;

      img {
        width: 40px;
        height: 40px;
        margin-right: 10px;
      }
    }
  }
  h4{
    color: #FB5938
  }
  ul,li{
    list-style none
    line-height: 30px
  }
  label{
    width: 100px;
    display: inline-block
    text-align: right;
    font-size:14px;
    color:rgba(55,57,76,1);
  }
  span{
    color: #939EAB;
  }
  .el-radio{
    text-align: left
    margin: 10px 0
  }
}
.audit_zone >>> .el-timeline-item__tail{
  border-left: 2px solid #FB5938;
}